@font-face {
  font-family: "iconfont";
  /* Project id 2651371 */
  src: url('//at.alicdn.com/t/font_2651371_ptz5siyamz8.woff2?t=1625461224700') format('woff2'), url('//at.alicdn.com/t/font_2651371_ptz5siyamz8.woff?t=1625461224700') format('woff'), url('//at.alicdn.com/t/font_2651371_ptz5siyamz8.ttf?t=1625461224700') format('truetype');
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-fangdajing:before {
  content: "\e633";
}
.search_input {
  height: 90rpx;
  padding: 10rpx;
}
.search_input navigator {
  display: flex;
  align-items: center;
  border-radius: 40rpx;
  height: 100%;
  font-size: 40rpx;
  background-color: white;
  border: 1rpx solid var(--fontcolor);
  color: var(--fontcolor);
}
.search_input navigator i {
  margin: 20rpx;
  color: var(--fontcolor);
  font-size: 40rpx;
}
