/* compoment/list/list.wxss */
@import "../../style/font.wxss";
.cont{
    border: 1rpx solid black;
    height: 80rpx;
    padding: 15rpx;
    border-radius: 20rpx;
    margin: 20rpx;
    color: #666;
}
.item{
    position: relative;
}
.cont .item .pos{
